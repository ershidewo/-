<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>servlet.payServlet</servlet-name>
        <servlet-class>servlet.payServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>servlet.resultServlet</servlet-name>
        <servlet-class>servlet.resultServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>servlet.shopServlet</servlet-name>
        <servlet-class>servlet.shopServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>servlet.searchServlet</servlet-name>
        <servlet-class>servlet.searchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servlet.searchServlet</servlet-name>
        <url-pattern>/searchServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>servlet.shopServlet</servlet-name>
        <url-pattern>/shopServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>servlet.resultServlet</servlet-name>
        <url-pattern>/resultServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>servlet.payServlet</servlet-name>
        <url-pattern>/payServlet</url-pattern>
    </servlet-mapping>
</web-app>